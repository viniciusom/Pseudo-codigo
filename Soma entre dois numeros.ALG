Algoritmo "Soma entre dois números"
Var
   n1, n2, result: real
Inicio
      Escreval("Digite o primeiro número: ")
      Leia(n1)
      Escreval("Digite o segundo número: ")
      Leia(n2)
      
      result <- n1+n2
      
      Escreva("A soma de",n1," +",n2," é:",result,"!")
Fimalgoritmo